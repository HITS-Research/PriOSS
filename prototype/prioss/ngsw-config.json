{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/images/fb-instructions/*",
          "/assets/images/insta-instructions/*",
          "/assets/images/spot-instructions/*",
          "/assets/images/icons/*",
          "/assets/images/appflow/appflow_cropped.png",
          "/assets/images/facebook.png",
          "/assets/images/instagram.png",
          "/assets/images/spotify.png",
          "/assets/sample-data/spot_sampledata.zip",
          "/assets/sample-data/insta_sampledata.zip",
          "/assets/sample-data/face_sampledata.zip",
          "/assets/sql-wasm.wasm",
          "/assets/outline/wifi.svg",
          "/assets/outline/arrow-right.svg",
          "/assets/outline/link.svg",
          "/assets/outline/arrow-left.svg"
        ]
      }
    }
  ]
}
